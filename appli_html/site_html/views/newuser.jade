extends layout

block content
	h1= title
	form#formAddUser(name="adduser",method="post",action="/adduser")
		input#inputUserNom(type="text", placeholder="usernom", name="usernom")
		input#inputUserPrenom(type="text", placeholder="userprenom", name="userprenom")
		input#inputUserEmail(type="text", placeholder="useremail", name="useremail")
		input#inputUserSociete(type="text", placeholder="usersociete", name="usersociete")
		button#btnSubmit(type="submit") submit